{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules"
  ],
  "jsRules": {
    "no-console": false
  },
  "rules": {
    "indent": [
      true,
      "spaces",
      4
    ],
    "no-trailing-whitespace": [
      true,
      "ignore-comments"
    ],
    "no-constant-condition": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-console": false
  },
  "rulesDirectory": []
}